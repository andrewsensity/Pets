<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coursera.pets">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pets">
        <activity android:name="com.coursera.pets.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.coursera.pets.Contact"
            android:parentActivityName="com.coursera.pets.MainActivity" />
        <activity
            android:name="com.coursera.pets.About"
            android:parentActivityName="com.coursera.pets.MainActivity" />
        <activity
            android:name="com.coursera.pets.FavoritesPets"
            android:parentActivityName="com.coursera.pets.MainActivity"/>
    </application>

</manifest>